// 8. Write a C++ program to swap values of two int variables without using third variable
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter the values";
    cin>>a>>b;
    
    cout<<"after swapping a is "<<b;
    cout<<endl;
    cout<<"after swapping b is "<<a;
    return 0;
}
